<div class="container p-4">
     <div class="row">

          <div class="col-md-12">
              <div class="card text-center">
                  <div class="card-body" style="overflow: auto;">
                            <table class="table">
                            <thead>
                                <tr>
                                <th scope="col">RES_ID</th>
                                <th scope="col">CLN_ID</th>
                                <th scope="col">CTA_ID</th>
                                <th scope="col">DEV_ID</th>
                                <th scope="col">GRP_ID</th>
                                <th scope="col">ROL_ID</th>
                                <th scope="col">TOU_ID</th>
                                <th scope="col">RES_ACOM</th>
                                <th scope="col">RES_EXTRA</th>
                                <th scope="col">RES_ABONO1</th>
                                <th scope="col">RES_ABONO2</th>
                                <th scope="col">RES_ABONO3</th>
                                <th scope="col">RES_OBSERVACIONES</th>
                                <th scope="col">RES_FECHA_ABONO1</th>
                                <th scope="col">RES_FECHA_ABONO2</th>
                                <th scope="col">RES_FECHA_ABONO3</th>
                                <th scope="col">ELIMINAR</th>
                                <th scope="col">EDITAR</th>
                                </tr>
                            </thead>
                            <tbody>
                            {{#each verReserva}}
                                <tr>
                                <th scope="row">{{RES_ID}}</th>
                                <td>{{CLN_ID}}</td>
                                <td>{{CTA_ID}}</td>
                                <td>{{DEV_ID}}</td>
                                <td>{{GRP_ID}}</td>
                                <td>{{ROL_ID}}</td>
                                <td>{{TOU_ID}}</td>
                                <td>{{RES_ACOM}}</td>
                                <td>{{RES_EXTRA}}</td>
                                <td>{{RES_ABONO1}}</td>
                                <td>{{RES_ABONO2}}</td>
                                <td>{{RES_ABONO3}}</td>
                                <td>{{RES_OBSERVACIONES}}</td>
                                <td>{{RES_FECHA_ABONO1}}</td>
                                <td>{{RES_FECHA_ABONO2}}</td>
                                <td>{{RES_FECHA_ABONO3}}</td>
                                <td><a href="/reservas/eliminar_reserva/{{RES_ID}}" class="btn btn-danger">Eliminar</a></td>
                                <td><a href="/reservas/editar_reserva/{{RES_ID}}" class="btn btn-secondary">Editar</a></td>
                                </tr>
                            {{/each}}
                            </tbody>
                            </table>
                  </div>
              </div>
          </div>
       
     </div>
</div>