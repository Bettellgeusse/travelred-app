<div class="container p-4">
     <div class="row">

          <div class="col-md-14">
              <div class="card text-center">
                  <div class="card-body">
                            <table class="table">
                            <thead>
                                <tr>
                                <th scope="col">ID</th>
                                <th scope="col">NOMBRE</th>
                                <th scope="col">APELLIDO</th>
                                <th scope="col">CEDULA</th>
                                <th scope="col">EDAD</th>
                                <th scope="col">FECHA DE NACIMIENTO</th>
                                <th scope="col">CORREO</th>
                                <th scope="col">OBSERVACIONES</th>
                                <th scope="col">ELIMINAR</th>
                                <th scope="col">EDITAR</th>
                                </tr>
                            </thead>
                            <tbody>
                            {{#each vercliente}}
                                <tr>
                                <th scope="row">{{CLN_ID}}</th>
                                <td>{{CLN_NOMBRE}}</td>
                                <td>{{CLN_APELLIDO}}</td>
                                <td>{{CLN_CEDULA}}</td>
                                <td>{{CLN_EDAD}}</td>
                                <td>{{CLN_FECHANAC}}</td>
                                <td>{{CLN_CORREO}}</td>
                                <td>{{CLN_OBSERVACIONES}}</td>
                                <td><a href="/clientes/eliminar_cliente/{{CLN_ID}}" class="btn btn-danger">Eliminar</a></td>
                                <td><a href="/clientes/editar_cliente/{{CLN_ID}}" class="btn btn-secondary">Editar</a></td>
                                </tr>
                            {{/each}}
                            </tbody>
                            </table>
                  </div>
              </div>
          </div>
       
     </div>
</div>